using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace ConsoleApp1
{
    class TestDate
    {
        public void Funcs()
        {
            Date date1 = new Date();
            Date date2 = new Date(1,1,1950);
            date1.SetDay(1); // check setters
            date1.SetMonth(1);
            date1.SetYear(1864);

            Console.WriteLine(date1.GetDay());  // check getters
            Console.WriteLine(date1.GetMonth()); 
            Console.WriteLine(date1.GetYear());

            Console.WriteLine(date1.CompareTo(date2));

            Console.WriteLine(date1.TooString());

            Console.ReadLine();
        }
    }
}
