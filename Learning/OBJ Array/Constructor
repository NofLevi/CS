using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace ConsoleApp1
{
    class Points
    {
        private int _x;
        private int _y;

        public Points(int x,int y)
        {
            _x = x;
            _y = y;
        }

        public String Getter()
        {
            return "x =" + _x+"\ny = " + _y;

        }
    }
}
