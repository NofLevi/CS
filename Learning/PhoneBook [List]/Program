using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace PhoneBook
{
    class Program
    {
        static void Main(string[] args)
        {
            PhoneBook phonebook = new PhoneBook();
            phonebook.AddContact("hello", "0123456789");
            phonebook.AddContact("World", "9876543210");
            Console.WriteLine(phonebook.ToString());
            phonebook.DelContact("hello");
        }
    }
}
