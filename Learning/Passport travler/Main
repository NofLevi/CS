using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace ConsoleApp1123
{
    class Program
    {
        static void Main(string[] args)
        {
            Date date1 = new Date(1, 2, 3);
            Date date2 = new Date(5, 6, 7);
            Passport pass1 = new Passport("asd", 5, date1);
            Passport pass2 = new Passport(pass1);
            pass1.SetExpiryDate(date2);
            Console.WriteLine(pass1.GetExpiryDate().GetDay());
            Console.WriteLine(pass2.GetExpiryDate().GetDay());
        }
    }
}
