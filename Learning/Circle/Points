using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace ConsoleApp1
{
    class Points
    {
        private int _x;
        private int _y;

        public Points(int x,int y)
        {
            _x = x;
            _y = y;
        }

        public int GetterX(){ return _x; }

        public int GetterY() { return _y; }

        public void Setter(int x ,int y) { _x = _x; _y = y; }
    }
}
